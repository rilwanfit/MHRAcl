        "rilwanfit/MHRAcl":                   "1.*"
    },
    "suggests": {
        "rilwanfit/MHRUser":                "zfc-user provides a good default setup to get started with bjyauthorize",
    },
    "autoload": {
        "psr-0": {
            "MHRAcl\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
